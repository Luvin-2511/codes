#include <iostream>
using namespace std;

int main() {
    int num1, num2, num3;
    cout << "Please input three numbers: ";
    cin >> num1 >> num2 >> num3;

    if (num1 >= num2 && num1 >= num3) {
        cout << num1 << " stands out as the largest." << endl;
    } else if (num2 >= num1 && num2 >= num3) {
        cout << num2 << " stands out as the largest." << endl;
    } else {
        cout << num3 << " stands out as the largest." << endl;
    }

    return 0;
}
