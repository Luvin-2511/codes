#include <iostream>
#include <string>
using namespace std;

void concatenateStrings(string* str1, string* str2, string* result) {
    *result = *str1 + *str2;
}

int main() {
    string str1 = "Hello";
    string str2 = "World!";
    string result;

    concatenateStrings(&str1, &str2, &result);

    cout << "The concatenated string is: " << result << endl;

    return 0;
}
