#include <iostream>
using namespace std;

int main() {
    int number;
    cout << "Enter a number to generate its multiplication table: ";
    cin >> number;
    int i = 1;
    cout << "Multiplication table of " << number << ":" << endl;
    while (i <= 10) {
        cout << number << " * " << i << " = " << (number * i) << endl;
        i++;
    }
    return 0;
}
